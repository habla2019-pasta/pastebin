    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "eslint": "^7.22.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-jest-dom": "^3.7.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
