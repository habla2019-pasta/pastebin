  "description": "Client side webapp for pasting",
  "sideEffects": false,
  "scripts": {
    "start": "webpack serve",
    "check": "flow",
    "build": "webpack",
    "prebuild": "yarn",
    "build:watch": "webpack --watch",
    "build:prod": "webpack --mode production",
    "prebuild:prod": "yarn",
    "delete": "rm -rf coverage/ dist/ node_modules/ yarn-error.log",
