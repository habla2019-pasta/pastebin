    "pretest": "rm -rf coverage",
    "test:watch": "rm -rf coverage && jest --coverage=false --watchAll",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ignore-path .gitignore .",
    "codecov": "codecov",
    "precodecov": "yarn build",
    "lhci": "lhci autorun",
    "prelhci": "yarn build:prod"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
