  "description": "Client side webapp for pasting",
  "sideEffects": false,
  "scripts": {
    "start": "webpack serve",
    "check": "flow",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:prod": "webpack --mode production",
    "delete": "rm -rf node_modules/ && rm -rf dist/ && rm -f yarn-error.log && rm -rf coverage",
